generator client {
  provider = "prisma-client-js"
  output   = "../src/generated/prisma"
}

datasource db {
  provider = "postgresql"
  url      = env("DATABASE_URL")
}

model Tutor {
  id                  Int    @id @default(autoincrement())
  institution         String
  subjects            String
  year                String
  name                String
  email               String @unique
  location            String
  sampleTeachingVideo String
  subjectToTeach      String
  userId              String
}
